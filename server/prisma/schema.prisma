generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:1111@localhost:5432/Kevych"
}

model User {
  id    Int     @id @default(autoincrement())
  full_name String
  age Int
  email String  @unique
  password  String
  password_salt String
  phone String
}

